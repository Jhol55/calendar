{
  "name": "wazzy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:studio": "prisma studio --port 5555",
    "test:jsdom": "jest --config jest.config.jsdom.ts",
    "test:node": "cross-env NODE_ENV=test DATABASE_URL=postgresql://postgres:123456@localhost:5433/wazzy_test jest --config jest.config.node.ts",
    "test:node:watch": "cross-env NODE_ENV=test DATABASE_URL=postgresql://postgres:123456@localhost:5433/wazzy_test jest --config jest.config.node.ts --watch",
    "test:watch": "jest --watch",
    "test:db:up": "docker compose -f docker-compose.test.yml up -d --wait",
    "test:db:down": "docker compose -f docker-compose.test.yml down",
    "test:db:migrate": "cross-env DATABASE_URL=postgresql://postgres:123456@localhost:5433/wazzy_test prisma migrate deploy",
    "test:db:push": "cross-env DATABASE_URL=postgresql://postgres:123456@localhost:5433/wazzy_test prisma db push",
    "test:integration": "node ./scripts/tests/integration.js",
    "test:integration:ci": "cross-env NODE_ENV=test jest --config jest.config.node.ts --ci --testPathIgnorePatterns=nothing",
    "test:integration:knapsack": "cross-env NODE_ENV=test npx @knapsack-pro/jest@latest --config jest.config.node.ts --ci",
    "format": "prettier --write .",
    "commit": "cz",
    "prepare": "husky install && git config --local core.editor cat && git config core.hooksPath .husky/_",
    "obfuscate": "node ./scripts/obfuscate.js",
    "build:obfuscated": "npm run build && npm run obfuscate",
    "docker-up:dev": "docker compose -f docker-compose.dev.yml up -d",
    "docker-up": "docker compose up -d",
    "worker": "tsx scripts/start-worker.js",
    "clear-queue": "node ./scripts/clear-webhook-queue.js",
    "clear-all": "node ./scripts/clear-all-jobs.js"
  },
  "dependencies": {
    "@codeium/react-code-editor": "^1.0.12",
    "@hookform/resolvers": "^5.2.2",
    "@number-flow/react": "^0.5.10",
    "@prisma/client": "^7.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@reactflow/node-resizer": "^2.2.14",
    "@stripe/stripe-js": "^8.5.3",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-query-persist-client": "^5.90.13",
    "@types/bull": "^4.10.4",
    "@types/node-cron": "^3.0.11",
    "bull": "^4.16.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "groq-sdk": "^0.37.0",
    "ioredis": "^5.8.2",
    "jose": "^6.1.2",
    "lucide-react": "^0.555.0",
    "next": "^16.0.6",
    "next-auth": "^4.24.13",
    "node-cron": "^4.2.1",
    "node-sql-parser": "^5.3.13",
    "better-playwright-mcp3": "^3.2.0",
    "playwright": "^1.57.0",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.53.1",
    "reactflow": "^11.11.4",
    "sharp": "^0.34.3",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "xior": "^0.8.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__react": "^10.0.1",
    "@types/uuid": "^10.0.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "husky": "^8.0.3",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prisma": "^6.13.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "webpack-obfuscator": "^3.5.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
